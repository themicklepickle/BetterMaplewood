{
  "name": "BetterMaplewood",
  "description": "BetterMaplewood, a browser extension to make your maplewood experience better",
  "version": "2.4.0",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://hosting.maplewood.com/*/main.aspx*"
      ],
      "js": [
        "injector.js"
      ],
      "run_at": "document_end",
      "all_frames": true
    }
  ],
  "web_accessible_resources": [
    "src/averageCalculator.js",
    "src/liveMarkbookCalculator.js"
  ],
  "browser_action": {
    "default_title": "BetterMaplewood",
    "default_popup": "src/popup.html"
  },
  "icons": {
    "16": "img/icon16.png",
    "32": "img/icon32.png",
    "96": "img/icon96.png",
    "128": "img/icon128.png",
    "196": "img/icon196.png"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "{b50c1372-c516-4876-bbc8-c6143d1df859}"
    }
  },
  "manifest_version": 2
}