{
    "name": "BetterMaplewood",
    "description": "BetterMaplewood, a chrome extension to make your maplewood experience better",
    "version": "1.0",

    "permissions": [
        "activeTab"
      ],

      "content_scripts": [
        {
          "matches": ["*://hosting.maplewood.com/*/main.aspx*"],
          "js": ["3rdparty/jquery.min.js","contentScript.js"],
          "run_at": "document_end",
          "all_frames": true
        }
      ],

    "browser_action": {
        "default_title": "BetterMaplewood"
    },

    "icons": { "32": "images/icon.png" },

    "manifest_version": 2
}